<Window x:Class="AI.RegisterInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AI"
        mc:Ignorable="d"
        Title="Artificial Intelligence - Register Info" Height="600" Width="1000" MaxWidth="1000" MaxHeight="600">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="361*"/>
            <ColumnDefinition Width="432*"/>
        </Grid.ColumnDefinitions>
        <Button Name="btnHome" Content="Home" HorizontalAlignment="Left" Margin="372,10,0,0" VerticalAlignment="Top" Width="160" Click="btnHome_Click" Grid.Column="1" Height="40" FontWeight="Bold" FontSize="20"/>
        <Button Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="373,316,0,0" VerticalAlignment="Top" Width="160" Click="btnLogin_Click" MaxWidth="160" Height="40" FontWeight="Bold" FontSize="20" Grid.ColumnSpan="2"/>
        <Label Name="lblResult" Content="Person created successfully!" HorizontalAlignment="Left" Margin="25,14,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Width="754" Height="36" FontSize="20" FontWeight="Bold"/>
    </Grid>
</Window>
